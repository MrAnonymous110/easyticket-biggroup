<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

   <!-- font end url -->
    <rule>
         <from>/index.html</from>
         <to type="forward">index.action</to>
    </rule>  
    
    <rule>
         <from>/login.html</from>
         <to type="forward">login.action</to>
    </rule>
    
     <rule>
         <from>/about.html</from>
         <to type="forward">about.action</to>
    </rule> 
    
     <rule>
         <from>/cart.html</from>
         <to type="forward">cart.action</to>
    </rule> 
    
      <rule>
         <from>/profile.html</from>
         <to type="forward">profile.action</to>
    </rule> 
    
    <rule>
         <from>/event.html</from>
         <to type="forward">event.action</to>
    </rule>
    
    <rule>
         <from>/event/([1-9]+)</from>
         <to type="forward">event.action?type=$1</to>
    </rule> 
    
    
    
</urlrewrite>
